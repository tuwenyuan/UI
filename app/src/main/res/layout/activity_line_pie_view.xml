<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">
        <twy.com.ui.view.pieview.LinePieView
            android:id="@+id/lpv"
            android:layout_width="match_parent"
            android:layout_height="190dp"
            app:centerTextColor="#000000"
            app:centerTextSize="24sp"
            app:circleWidth="20dp"
            app:dataTextColor="#ff00ff"
            app:dataTextSize="12sp" />
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center_horizontal"
            android:layout_marginBottom="10dp"
            android:orientation="horizontal">
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical">
                <LinearLayout
                    android:id="@+id/ll_dailishouyi"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical"
                    android:orientation="horizontal">
                    <View
                        android:id="@+id/v_dailishouyi_in"
                        android:layout_width="8dp"
                        android:layout_height="8dp"
                        android:background="#eb5553"/>
                    <TextView
                        android:id="@+id/tv_one_in"
                        android:layout_width="80dp"
                        android:layout_height="wrap_content"
                        android:textColor="#000000"
                        android:layout_marginLeft="6dp"
                        android:textSize="12sp"
                        android:text="代理收益"/>
                </LinearLayout>
                <LinearLayout
                    android:id="@+id/ll_huiyuanshouyi_in"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical"
                    android:layout_marginTop="10dp"
                    android:orientation="horizontal">
                    <View
                        android:id="@+id/v_huiyuanshouyi_in"
                        android:layout_width="8dp"
                        android:layout_height="8dp"
                        android:background="#eb5553"/>
                    <TextView
                        android:id="@+id/tv_four_in"
                        android:layout_width="80dp"
                        android:layout_height="wrap_content"
                        android:textColor="#000000"
                        android:layout_marginLeft="6dp"
                        android:textSize="12sp"
                        android:text="会员收益"/>
                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:orientation="vertical">
                <LinearLayout
                    android:id="@+id/ll_dailifenhong_in"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical"
                    android:orientation="horizontal">
                    <View
                        android:id="@+id/v_dailifenhong_in"
                        android:layout_width="8dp"
                        android:layout_height="8dp"
                        android:background="#eb5553"/>
                    <TextView
                        android:id="@+id/tv_two_in"
                        android:layout_width="80dp"
                        android:layout_height="wrap_content"
                        android:textColor="#000000"
                        android:layout_marginLeft="6dp"
                        android:textSize="12sp"
                        android:text="代理分红"/>
                </LinearLayout>
                <LinearLayout
                    android:id="@+id/ll_others_in"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical"
                    android:layout_marginTop="10dp"
                    android:orientation="horizontal">
                    <View
                        android:id="@+id/v_others_in"
                        android:layout_width="8dp"
                        android:layout_height="8dp"
                        android:background="#eb5553"/>
                    <TextView
                        android:id="@+id/tv_five_in"
                        android:layout_width="80dp"
                        android:layout_height="wrap_content"
                        android:textColor="#000000"
                        android:layout_marginLeft="6dp"
                        android:textSize="12sp"
                        android:text="其它"/>
                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical">
                <LinearLayout
                    android:id="@+id/ll_huiyuanfenhong_in"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical"
                    android:orientation="horizontal">
                    <View
                        android:id="@+id/v_huiyuanfenhong_in"
                        android:layout_width="8dp"
                        android:layout_height="8dp"
                        android:background="#eb5553"/>
                    <TextView
                        android:id="@+id/tv_three_in"
                        android:layout_width="80dp"
                        android:layout_height="wrap_content"
                        android:textColor="#000000"
                        android:layout_marginLeft="6dp"
                        android:textSize="12sp"
                        android:text="加权分红"/>
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
</layout>
